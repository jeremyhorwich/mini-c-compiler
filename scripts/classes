public class BinaryExpression : Expression 
{
	public Exprresion left;
	public Token operator;
	public Expression right;

	BinaryExpression(Exprresion _left, Token _operator, Expression _right)
	{
		left = _left;
		operator = _operator;
		right = _right;
	}
}

public class UnaryExpression : Expression 
{
	public Expression expression;
	public Token operator;

	UnaryExpression(Expression _expression, Token _operator)
	{
		expression = _expression;
		operator = _operator;
	}
}

